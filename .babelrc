{
    "presets": [
        [
            "@babel/preset-env",
            {
                "targets": {
                    "node": "6.10",
                    "esmodules": true
                }
            }
        ],
        [
            "@babel/preset-react"
        ]
    ],
    "plugins": [
        "@babel/plugin-proposal-object-rest-spread",
        "@babel/plugin-transform-runtime"
    ]
}